{
    "name": "xepozz/breadcrumbs-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "A small breadcrumbs bundle for Symfony5",
    "keywords": [
        "breadcrumbs",
        "symfony 5",
        "symfony"
    ],
    "require": {
        "php": ">=7.2",
        "symfony/framework-bundle": "^5.0",
        "symfony/templating": "^5.0"
    },
    "require-dev": {
        "twig/twig": "^3.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/twig-bridge": "^5.0",
        "phpunit/phpunit": "^8.5",
        "symfony/browser-kit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Xepozz\\BreadcrumbsBundle\\": "src",
            "Xepozz\\BreadcrumbsBundle\\Tests\\": "tests"
        }
    }
}
